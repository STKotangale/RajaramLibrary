	<parameter name="bookTypeId" class="java.lang.String"/>
	<parameter name="bookTypeName" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
    bd.accessionNo, 
    bd.classificationNumber,
    IFNULL( (SELECT ba.authorName FROM invt_book_author ba, invt_book bk WHERE bk.bookId = bd.bookIdF AND bk.authorIdF = ba.authorId), 'Null') AS authorName, 
    IFNULL( (SELECT bk.bookName FROM invt_book bk WHERE bk.bookId = bd.bookIdF), 'Null') AS bookName, 
    bd.edition, 
    IFNULL( (SELECT CONCAT(bp.publicationName,', ', bp.publicationAddress) FROM invt_book_publication bp, invt_book bk WHERE bk.bookId = bd.bookIdF AND bk.publicationIdF = bp.publicationId), 'Null') AS publicationName, 
    bd.publicationYear, 
    bd.numberOfPages, 
    IFNULL( (SELECT stdet.book_rate FROM invt_stockdetail stdet WHERE stdet.stockDetailId = bd.stockDetailIdF), 'Null') AS book_rate,
    IFNULL( (SELECT led.ledgerName FROM acc_ledger led, invt_stockdetail stdet, invt_stock stk WHERE stdet.stockDetailId = bd.stockDetailIdF AND stk.stock_id = stdet.stock_idF AND led.ledgerID = stk.ledgerIDF), 'Null') AS ledgerName, 
    IFNULL( (SELECT stk.invoiceNo FROM invt_stockdetail stdet, invt_stock stk WHERE stdet.stockDetailId = bd.stockDetailIdF AND stk.stock_id = stdet.stock_idF), 'Null') AS invoiceNo,
    IFNULL( (SELECT stk.invoiceDate FROM invt_stockdetail stdet, invt_stock stk WHERE stdet.stockDetailId = bd.stockDetailIdF AND stk.stock_id = stdet.stock_idF), 'Null') AS invoiceDate,  
    (SELECT 
        CASE 
            WHEN bda.bookLost = 'Y' THEN 'Lost' 
            WHEN bda.bookScrap = 'Y' THEN 'Scrap' 
            WHEN bda.book_return = 'Y' THEN 'Return' 
            WHEN bda.bookIssue = 'Y' THEN 'Issued' 
            WHEN bda.bookIssue = 'N' THEN 'Available' 
            ELSE '' 
        END AS statusText 
     FROM invt_book_details bda 
     WHERE bda.bookDetailId = bd.bookDetailId 
    ) AS StatusText
FROM invt_book_details bd, invt_book bk 
WHERE 
bd.bookIdF = bk.bookId AND bk.bookTypeIdF = $P{bookTypeId}
ORDER BY bd.accessionNo;]]>
	</queryString>
