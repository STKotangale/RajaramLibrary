	<parameter name="startDate" class="java.lang.String"/>
	<parameter name="endDate" class="java.lang.String"/>
	<parameter name="memberId" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
    stk.invoiceDate, 
    stk.invoice_time, 
    gmem.libGenMembNo ,
    concat(gmem.firstName, ' ', gmem.middleName, ' ', gmem.lastName) as memName, 
    bk.bookName, 
    ifnull((SELECT bdet.accessionNo FROM invt_book_details bdet WHERE bdet.bookDetailId = stdet.bookDetailIdf), 'Null') as accessionNo, 
    ifnull((SELECT bdet.classificationNumber FROM invt_book_details bdet WHERE bdet.bookDetailId = stdet.bookDetailIdf), 'Null') as classificationNumber, 
    ifnull( DATE_FORMAT( ADDDATE( date(concat(SUBSTRING(stk.invoiceDate, 7, 4), '-', SUBSTRING(stk.invoiceDate, 4, 2), '-', SUBSTRING(stk.invoiceDate, 1, 2))),  (SELECT bookDays FROM invt_config where srno = 1) ), '%d-%m-%Y')   , 'Null') as returnDate,
    date(concat(SUBSTRING(stk.invoiceDate, 7, 4), '-', SUBSTRING(stk.invoiceDate, 4, 2), '-', SUBSTRING(stk.invoiceDate, 1, 2))) as sortDate 
FROM 
    invt_stockdetail stdet, 
    invt_stock stk, 
    auth_general_members gmem, 
    invt_book bk 
WHERE 
    stk.stock_type = 'A2' AND stdet.stock_idF = stk.stock_id AND gmem.memberId = stk.memberIdF AND bk.bookId = stdet.book_idF 
AND date(concat(SUBSTRING(stk.invoiceDate, 7, 4), '-', SUBSTRING(stk.invoiceDate, 4, 2), '-', SUBSTRING(stk.invoiceDate, 1, 2))) 
        BETWEEN $P{startDate} AND $P{endDate} AND stk.memberIdF = $P{memberId}
        ORDER BY sortDate]]>
	</queryString>
